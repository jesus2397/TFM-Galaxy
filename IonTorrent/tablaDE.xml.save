<tool id="show_de" name="Show taxa differentially abundance" version="0.1.0" python_template_version="3.5">
    <requirements>
    </requirements>
    <command detect_errors="exit_code" interpreter='Rscript'>
        tablaDE.R -r '$input1'
   </command>
